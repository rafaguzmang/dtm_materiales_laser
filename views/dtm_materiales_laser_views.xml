<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <data>
        <record id="dtm_materiales_lase_tree" model="ir.ui.view">
            <field name="name">dtm_materiales_lase_tree</field>
            <field name="model">dtm.materiales.laser</field>
            <field name="arch" type="xml">
                <tree create="false">
                    <field name="material"/>
                    <field name="calibre"/>
                    <field name="largo"/>
                    <field name="ancho"/>
                    <field name="cantidad"/>
                    <button name="acction_less" type="object" class="fa fa-minus oe_highlight"/> 
                </tree>
            </field>
        </record>
    
        <record id="dtm_materiales_lase_accion" model="ir.actions.act_window">
            <field name="name">Materiales Laser</field>
            <field name="res_model">dtm.materiales.laser</field>
            <field name="view_mode">tree</field>
        </record>
        <menuitem id="dtm_materiales_laser" parent="" action="dtm_materiales_lase_accion"/>
    </data>
   
</odoo>